files:

  - path: "../../data/datasets/Math/{dataset}.json"
    description: "Initial dataset with problem statements and answers"
    produced_by: null
    consumed_by: get_variables_for_problems.py

  - path: "../../data/calculation/{time}/{dataset}_problem_variables.json"
    description: "Extract or generate variables for problems"
    produced_by: get_variables_for_problems.py
    consumed_by: ["qwen-structured.py", "var_descriptions_for_every_statement.py"]

  - path: "../../data/calculation/{time}/{dataset}_st.json"
    description: "Initial dataset with structured solutions"
    produced_by: qwen-structured.py
    consumed_by: ["create_lemmas_csv.py", "formalize.py"]

  - path: "../../data/calculation/{time}/{dataset}_lemmas.csv"
    description: "Lemmas of structured solutions to csv"
    produced_by: create_lemmas_csv.py
    consumed_by: var_descriptions_for_every_statement.py

  - path: "../../data/calculation/{time}/{dataset}_full_descriptions"
    description: "Full independent descriptions of every problem and lemma"
    produced_by: var_descriptions_for_every_statement.py
    consumed_by: var_descriptions.py

  - path: "../../data/calculation/{time}/{dataset}_descriptions"
    description: "Correct descriptions of lemmas with context of problem and previous lemmas"
    produced_by: var_descriptions.py
    consumed_by: lemmas_from_structured.py

  - path: "../../data/calculation/{time}/{dataset}_uncorrected_th_st.csv"
    description: "Initial dataset with formal theorem statements and structured solutions"
    produced_by: formalize.py
    consumed_by: correct_theorems.py

  - path: "../../data/calculation/{time}/{dataset}_uncorrected_lemmas_st.csv"
    description: "Formal lemmas generated by proof steps, unproved"
    produced_by: lemmas_from_structured.py
    consumed_by: ["correct_unproved_lemmas.py", "correct_theorems.py", "report_by_lemmas.py", "report_on_change.py"]

  - path: "../../data/calculation/{time}/{dataset}_th_st.csv"
    description: "Initial dataset with formal corrected theorem statements"
    produced_by: correct_theorems.py
    consumed_by: correct_unproved_lemmas.py
    
  - path: "../../data/calculation/{time}/{dataset}_lemmas_unproved_corrected"
    description: "Fix some syntax and semantic errors in statements of unproved lemmas"
    produced_by: correct_unproved_lemmas.py
    consumed_by: ["kimina_prover_for_lemmas.py", "report_by_lemmas.py", "report_on_change.py", "check_correctness.py"]

  - path: "../../data/calculation/{time}/{dataset}_lemmas_proved"
    description: "Formal lemmas generated by proof steps, proved"
    produced_by: kimina_prover_for_lemmas.py
    consumed_by: ["report_by_lemmas.py", "report_on_change.py", "assemble_proofs_from_lemmas.py"]

  - path: "../../data/result/verify_solution/{time}/{dataset}"
    description: "Tentative theorems with proofs"
    produced_by: assemble_proofs_from_lemmas.py
    consumed_by: ["verify_multiple_proofs.py", "report_by_lemmas.py", "report_on_change.py"]

  - path: "../../data/result/verify_solution/{time}/{dataset}/verification_report.html"
    description: "Report by lemmas"
    produced_by: report_by_lemmas.py
    consumed_by: null
  
  - path: "../../data/result/verify_solution/{time}/{dataset}/report_on_change.html"
    description: "Report on change"
    produced_by: report_on_change.py
    consumed_by: null
